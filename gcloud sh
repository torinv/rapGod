export BUCKET_NAME=rapgodbucket
export JOB_NAME="testjob1"
export JOB_DIR=gs://$BUCKET_NAME/$JOB_NAME
export REGION=us-east1

gcloud ml-engine jobs submit training testjob6 \
  --job-dir gs://rapgodbucket/testjob6 \
  --runtime-version 1.0 \
  --module-name trainer.model \
  --package-path ./trainer \
  --region us-east1 \
  --config=trainer/cloudml-gpu.yaml \
  -- \
  --train-file gs://rapgodbucket/test1.txt